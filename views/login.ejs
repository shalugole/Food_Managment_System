    .box input[type="text"],.box input[type="password"]{
        border: 0;
        background: none;
        display: block;
        margin: 15px auto;
        text-align: center;
        border: 2px solid #3498db;
        padding: 10px 10px;
        width: 200px;
        outline: none;
        color: white;
        border-radius: 30px;
        transition: 0.25s;
    }
    .box input[type="text"]:focus,.box input[type="password"]:focus{
        width: 350px;
        border-color: #2ecc71;

    }
    .box input[type="submit"]{
        border: 0;
        background: none;
        display: block;
        margin: 20px auto;
        text-align: center;
        border: 2px solid #3498db;
        padding: 10px 40px;
        outline: none;
        color: white;
        cursor: pointer;
        border-radius: 30px;
        transition: 0.25s;
    }
     .box input[type="submit"]:hover{
        background: #2ecc71;
     }  
